pipeline {
  agent none
  stages {
    stage('Back-end') {
      agent {
        docker { 
            image 'maven:3.8.1-adoptopenjdk-11'
            
               }
      }
      steps {
        dir('/var/lib/jenkins/workspace/jenkins-example-docker/pom.xml')
        sh 'mvn --version'
        sh 'mvn package'
        sh 'mvn install'
        sh 'mvn package'
      }
    
    }
    stage('Front-end') {
      agent {
        docker { image 'node:16-alpine' }
      }
      steps {
        sh 'node --version'
      }
    }
  }
}
